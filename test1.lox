fun median(a, b, c) {
    if ((a <= b) and (b <= c)) return b;  
    if ((a <= c) and (c <= b)) return c;  
    if ((b <= a) and (a <= c)) return a;  
    if ((b <= c) and (c <= a)) return c;  
    if ((c <= a) and (a <= b)) return a;  
    return b;                            			
}
var x = median(2, 5, 9);
print x;
